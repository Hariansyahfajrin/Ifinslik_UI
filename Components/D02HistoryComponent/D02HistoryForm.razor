@if (row != null)
{
		<RadzenTemplateForm TItem="D02HistoryModel" Data="@row" Submit=@OnSubmit>
			<RadzenStack>
				<RadzenRow Gap="8">
				@* <RadzenButton ButtonType="ButtonType.Submit" ButtonStyle="ButtonStyle.Primary" Text="Save" /> *@

					<RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Back" Click="@Back" />
				</RadzenRow>


				<RadzenStack>
				@if (row != null)
				{
							<RadzenRow>
								<FormFieldTextBox Label="Cif" Name="Cif" @bind-Value="@row.Cif" Max="50" Required="true" Disabled="true" />
								<FormFieldTextBox Label="NomorIdentitasBadanUsaha" Name="Nomor Identitas Badan Usaha"
									@bind-Value="@row.NomorIdentitasBadanUsaha" Max="50" Required="true" Disabled="true" />
								<FormFieldTextBox Label="NamaBadanUsaha" Name="Nama Badan Usaha" @bind-Value="@row.NamaBadanUsaha" Max="50"
									Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
																<FormFieldTextBox Label="Period" Name="Period" @bind-Value="@row.Period" Max="50" Required="true"
									Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldButtonLookup Name="KodeJenisBadanUsaha" Label="Kode Jenis Badan Usaha" Target="JenisBadanUsahaLookup"
									@bind-Value="@row.KodeJenisBadanUsaha" @bind-Text="@row.KodeJenisBadanUsahaDesc" Required="true"
									Disabled="true" />
								<FormFieldTextBox Label=" Tempat Pendirian" Name="TempatPendirian" @bind-Value="@row.TempatPendirian" Max="50"
									Required="true" Disabled="true" />
								<FormFieldTextBox Label="Nomor Akta Pendirian" Name="NomorAktaPendirian" @bind-Value="@row.NomorAktaPendirian"
									Max="50" Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldDatePicker Label="Tanggal Akta Pendirian" Name="TanggalAktaPendirian"
									@bind-Value="@row.TanggalAktaPendirian" Required="true" Disabled="true" />
								<FormFieldTextBox Label="Nomor Akta Perubahan Terakhit" Name="NomorAktaPerubahanTerakhir"
									@bind-Value="@row.NomorAktaPerubahanTerakhir" Max="50" Required="true" Disabled="true" />
								<FormFieldDatePicker Label="Tanggal Akta Perubahaan Terakhir" Name="TanggalAktaPerubahanTerakhir"
									@bind-Value="@row.TanggalAktaPerubahanTerakhir" Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldTextBox Label="Nomor Telepon" Name="NomorTelepon" @bind-Value="@row.NomorTelepon" Max="50"
									Required="true" Disabled="true" />
								<FormFieldTextBox Label="NomorHandphone" Name="NomorHandphone" @bind-Value="@row.NomorHandphone" Max="50"
									Required="true" Disabled="true" />
								<FormFieldTextBox Label="Email" Name="Email" @bind-Value="@row.Email" Max="50" Required="true"
									Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldTextArea Label="Alamat" Name="Alamat" @bind-Value="@row.Alamat" Max="50" Required="true"
									Disabled="true" />
								<FormFieldTextBox Label="Kelurahan" Name="Kelurahan" @bind-Value="@row.Kelurahan" Max="50" Required="true"
									Disabled="true" />
								<FormFieldTextBox Label="Kecamatan" Name="Kecamatan" @bind-Value="@row.Kecamatan" Max="50" Required="true"
									Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldButtonLookup Name="KodeDati2" Label="Kode Dati 2" Target="KodeDati2Lookup"
									@bind-Value="@row.KodeDati2" @bind-Text="@row.KodeDati2Desc" Required="true" Disabled="true" />
								<FormFieldTextBox Label="Kode Pos" Name="KodePos" @bind-Value="@row.KodePos" Max="50" Required="true"
									Disabled="true" />
								<FormFieldButtonLookup Name="KodeNegaraDomisili" Label="Negara Domisili" Target="NegaraDomisiliLookup"
									@bind-Value="@row.KodeNegaraDomisili" @bind-Text="@row.KodeNegaraDomisiliDesc" Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldButtonLookup Name="KodeBidangUsaha" Label="Kode Bidang Usaha" Target="BidangUsahaLookup"
									@bind-Value="@row.KodeBidangUsaha" @bind-Text="@row.KodeBidangUsahaDesc" Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="KodeHubunganDenganPelapor" Label="Hubungan Dengan Pelapor"
									Target="HubunganDenganPelaporLookup" @bind-Value="@row.KodeHubunganDenganPelapor"
									@bind-Text="@row.KodeHubunganDenganPelaporDesc" Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="MelanggarBmpk" Label="Melanggar BMPK" Target="MelangarMbpkLookup"
									@bind-Value="@row.MelanggarBmpk" @bind-Text="@row.MelanggarBmpkDesc" Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldButtonLookup Name="MelampauiBmpk" Label="Melampaui BMPK" Target="MelampauiBmpkLookup"
									@bind-Value="@row.MelampauiBmpk" @bind-Text="@row.MelampauiBmpkDesc" Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="GoPublic" Label="Go Public" Target="GoPublicLookup" @bind-Value="@row.GoPublic"
									@bind-Text="@row.GoPublicDesc" Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="KodeGolonganDebitur" Label="Golongan Debitur" Target="GolonganDebiturLookup"
									@bind-Value="@row.KodeGolonganDebitur" @bind-Text="@row.KodeGolonganDebiturDesc" Required="true"
									Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldTextBox Label="Peringkat Debitur" Name="PeringkatDebitur" @bind-Value="@row.PeringkatDebitur" Max="50"
									Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="LembagaPemeringkat" Label="Lembaga Pemeringkat" Target="LembagaPemeringkatLookup"
									@bind-Value="@row.GoPublic" @bind-Text="@row.LembagaPemeringkatDesc" Required="true" Disabled="true" />
								<FormFieldDatePicker Label="Tanggal Pemeringkatan" Name="TanggalPemeringkatan"
									@bind-Value="@row.TanggalPemeringkatan" Required="true" Disabled="true" />
							</RadzenRow>
							<RadzenRow>
								<FormFieldTextBox Label="Nama Grup Debitur" Name="NamaGrupDebitur" @bind-Value="@row.NamaGrupDebitur" Max="50"
									Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="KodeKantorCabang" Label="Kantor Cabang" Target="KantorCabangLookup"
									@bind-Value="@row.KodeKantorCabang" @bind-Text="@row.SysCompanyKantorCabang" Required="true" Disabled="true" />
								<FormFieldButtonLookup Name="OperasiData" Label="Operasi Data" Target="OperasiDataLookup"
									@bind-Value="@row.OperasiData" @bind-Text="@row.OperasiDataDesc" Required="true" Disabled="true" />
							</RadzenRow>

				}
				</RadzenStack>
			</RadzenStack>
		</RadzenTemplateForm>
}
<SingleSelectLookup @ref="@JenisBadanUsahaLookup" TItem="MasterOJKReferenceModel" LoadData="LoadJenisBadanUsahaLookup"
	Title="Code List" Select="(select) => {
								row.KodeJenisBadanUsaha            = select.Code;
								row.KodeJenisBadanUsahaOJKCode     = select.OJKCode;
								row.KodeJenisBadanUsahaDesc        = select.Description;
							}">
	<RadzenDataGridColumn TItem="MasterOJKReferenceModel" Property="OJKCode" Title="OJK Code" />
	<RadzenDataGridColumn TItem="MasterOJKReferenceModel" Property="Description" Title="Description" />
</SingleSelectLookup>
