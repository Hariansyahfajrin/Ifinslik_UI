@if (row != null)
{
	<RadzenTemplateForm TItem="D02Model" Data="@row" Submit=@OnSubmit>
		<RadzenStack>
			<RadzenRow Gap="8">
				<RoleAccess Code="RC0000020">
					<RadzenButton ButtonType="ButtonType.Submit" ButtonStyle="ButtonStyle.Primary" Text="Save" />
				</RoleAccess>
				<RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Back" Click="@Back" />
			</RadzenRow>
			<RadzenStack>
				@if (row != null)
				{
					<RadzenRow>
						<FormFieldTextBox Label="CIF" Name="Cif" @bind-Value="@row.Cif" Max="40" Required="true" />
						<FormFieldTextBox Label="Nomor Identitas Badan Usaha" Name="Nomor Identitas Badan Usaha"
							@bind-Value="@row.NomorIdentitasBadanUsaha" Max="16" Required="true" />
						<FormFieldTextBox Label="Nama Badan Usaha" Name="Nama Badan Usaha" @bind-Value="@row.NamaBadanUsaha" Max="300"
							Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeJenisBadanUsaha" Label="Kode Jenis Badan Usaha" Target="JenisBadanUsahaLookup"
							@bind-Value="@row.KodeJenisBadanUsaha" @bind-Text="@row.KodeJenisBadanUsahaDesc" Required="true" />
						<FormFieldTextBox Label="Tempat Pendirian" Name="TempatPendirian" @bind-Value="@row.TempatPendirian" Max="100"
							Required="true" />
						<FormFieldTextBox Label="Nomor Akta Pendirian" Name="NomorAktaPendirian" @bind-Value="@row.NomorAktaPendirian"
							Max="60" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldDatePicker Label="Tanggal Akta Pendirian" Name="TanggalAktaPendirian"
							@bind-Value="@row.TanggalAktaPendirian" Required="true" />
						<FormFieldTextBox Label="Nomor Akta Perubahan Terakhir" Name="NomorAktaPerubahanTerakhir"
							@bind-Value="@row.NomorAktaPerubahanTerakhir" Max="60" Required="true" />
						<FormFieldDatePicker Label="Tanggal Akta Perubahaan Terakhir" Name="TanggalAktaPerubahanTerakhir"
							@bind-Value="@row.TanggalAktaPerubahanTerakhir" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldTextBox Label="Nomor Telepon" Name="NomorTelepon" @bind-Value="@row.NomorTelepon" Max="30"
							Required="true" />
						<FormFieldTextBox Label="Nomor Handphone" Name="NomorHandphone" @bind-Value="@row.NomorHandphone" Max="30"
							Required="true" />
						<FormFieldTextBox Label="Email" Name="Email" @bind-Value="@row.Email" Max="300" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldTextArea Label="Alamat" Name="Alamat" @bind-Value="@row.Alamat" Max="600" Required="true" />
						<FormFieldTextBox Label="Kelurahan" Name="Kelurahan" @bind-Value="@row.Kelurahan" Max="100" Required="true" />
						<FormFieldTextBox Label="Kecamatan" Name="Kecamatan" @bind-Value="@row.Kecamatan" Max="100" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeDati2" Label="Kode Dati 2" Target="KodeDati2Lookup"
							@bind-Value="@row.KodeDati2" @bind-Text="@row.KodeDati2Desc" Required="true" />
						<FormFieldTextBox Label="Kode Pos" Name="KodePos" @bind-Value="@row.KodePos" Max="10" Required="true" />
						<FormFieldButtonLookup Name="KodeNegaraDomisili" Label="Negara Domisili" Target="NegaraDomisiliLookup"
							@bind-Value="@row.KodeNegaraDomisili" @bind-Text="@row.KodeNegaraDomisiliDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeBidangUsaha" Label="Kode Bidang Usaha" Target="BidangUsahaLookup"
							@bind-Value="@row.KodeBidangUsaha" @bind-Text="@row.KodeBidangUsahaDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeHubunganDenganPelapor" Label="Hubungan Dengan Pelapor"
							Target="HubunganDenganPelaporLookup" @bind-Value="@row.KodeHubunganDenganPelapor"
							@bind-Text="@row.KodeHubunganDenganPelaporDesc" Required="true" />
						<FormFieldButtonLookup Name="MelanggarBmpk" Label="Melanggar BMPK" Target="MelangarMbpkLookup"
							@bind-Value="@row.MelanggarBmpk" @bind-Text="@row.MelanggarBmpkDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="MelampauiBmpk" Label="Melampaui BMPK" Target="MelampauiBmpkLookup"
							@bind-Value="@row.MelampauiBmpk" @bind-Text="@row.MelampauiBmpkDesc" Required="true" />
						<FormFieldButtonLookup Name="GoPublic" Label="Go Public" Target="GoPublicLookup" @bind-Value="@row.GoPublic"
							@bind-Text="@row.GoPublicDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeGolonganDebitur" Label="Golongan Debitur" Target="GolonganDebiturLookup"
							@bind-Value="@row.KodeGolonganDebitur" @bind-Text="@row.KodeGolonganDebiturDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldTextBox Label="Peringkat Debitur" Name="PeringkatDebitur" @bind-Value="@row.PeringkatDebitur" Max="12"
							Required="true" />
						<FormFieldButtonLookup Name="LembagaPemeringkat" Label="Lembaga Pemeringkat" Target="LembagaPemeringkatLookup"
							@bind-Value="@row.GoPublic" @bind-Text="@row.LembagaPemeringkatDesc" Required="true" />
						<FormFieldDatePicker Label="Tanggal Pemeringkatan" Name="TanggalPemeringkatan"
							@bind-Value="@row.TanggalPemeringkatan" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldTextBox Label="Nama Grup Debitur" Name="NamaGrupDebitur" @bind-Value="@row.NamaGrupDebitur" Max="300"
							Required="true" />
						<FormFieldButtonLookup Name="KodeKantorCabang" Label="Kantor Cabang" Target="KantorCabangLookup"
							@bind-Value="@row.KodeKantorCabang" @bind-Text="@row.SysCompanyKantorCabang" Required="true" />
						<FormFieldButtonLookup Name="OperasiData" Label="Operasi Data" Target="OperasiDataLookup"
							@bind-Value="@row.OperasiData" @bind-Text="@row.OperasiDataDesc" Required="true" />
					</RadzenRow>

				}
			</RadzenStack>
		</RadzenStack>
	</RadzenTemplateForm>
}
<SingleSelectLookup @ref="@JenisBadanUsahaLookup" TItem="MasterOJKReferenceModel" LoadData="LoadJenisBadanUsahaLookup"
	Title="Jenis Badan Usaha List " Select="(select) => {
								row.KodeJenisBadanUsaha            = select.Code;
								row.KodeJenisBadanUsahaOJKCode     = select.OJKCode;
								row.KodeJenisBadanUsahaDesc        = select.Description;
							}">
	<RadzenDataGridColumn TItem="MasterOJKReferenceModel" Property="OJKCode" Title="OJK Code" />
	<RadzenDataGridColumn TItem="MasterOJKReferenceModel" Property="Description" Title="Description" />
</SingleSelectLookup>
