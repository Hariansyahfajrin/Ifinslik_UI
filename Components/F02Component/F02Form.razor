@if (row != null)
{
	<RadzenTemplateForm TItem="F02Model" Data="@row" Submit=@OnSubmit>
		<RadzenStack>
			<RadzenRow Gap="8">
				<RoleAccess Code="RC0000028">
					<RadzenButton ButtonType="ButtonType.Submit" ButtonStyle="ButtonStyle.Primary" Text="Save" />
				</RoleAccess>
				<RadzenButton ButtonStyle="ButtonStyle.Danger" Text="Back" Click="@Back" />
			</RadzenRow>
			<RadzenStack>
				@if (row != null)
				{

					<RadzenRow>
						<FormFieldTextBox Label="Nomor Rekening" Name="NomorRekening" @bind-Value="@row.NomorRekening" Max="50"
							Required="true" />
						<FormFieldButtonLookup Name="CIF" Label="Cif" Target="CifLookup" @bind-Value="@row.Cif" @bind-Text="@row.Cif"
							Required="true" />
						<FormFieldNumeric Label="Sequence Debitur" Name="SequenceDebitur" @bind-Value="@row.SequenceDebitur"
							Required="true" Format="#,#.00" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeSifatKredit" Label="Sifat Kredit" Target="SifatKreditLookup"
							@bind-Value="@row.KodeSifatKredit" @bind-Text="@row.KodeSifatKreditDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeJenisKredit" Label="Jenis Kredit" Target="JenisKreditLookup"
							@bind-Value="@row.KodeJenisKredit" @bind-Text="@row.KodeJenisKreditDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeSkim" Label="SKIM" Target="SkimLookup" @bind-Value="@row.KodeSkim"
							@bind-Text="@row.KodeSkimDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldTextBox Label="Nomor Akad Awal" Name="NomorAkadAwal" @bind-Value="@row.NomorAkadAwal" Max="100"
							Required="true" />

						<FormFieldDatePicker Label="Tanggal Akad Awal" Name="TanggalAkadAwal" @bind-Value="@row.TanggalAkadAwal"
							Required="true" />
						<FormFieldTextBox Label="Nomor Akad Akhir" Name="NomorAkadAkhir" @bind-Value="@row.NomorAkadAkhir" Max="100"
							Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldDatePicker Label="Tanggal Akad Akhir" Name="TanggalAkadAkhir" @bind-Value="@row.TanggalAkadAkhir"
							Required="true" />


						<FormFieldButtonLookup Name="BaruPerpanjangan" Label="Baru Perpanjangan" Target="BaruPerpanjanganLookup"
							@bind-Value="@row.BaruPerpanjangan" @bind-Text="@row.BaruPerpanjanganDesc" Required="true" />
						<FormFieldDatePicker Label="Tanggal Awal Kredit" Name="TanggalAwalKredit" @bind-Value="@row.TanggalAwalKredit"
							Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldDatePicker Label="Tanggal Mulai" Name="TanggalMulai" @bind-Value="@row.TanggalMulai"
							Required="true" />


						<FormFieldDatePicker Label="Tanggal Jatuh Tempo" Name="TanggalJatuhTempo" @bind-Value="@row.TanggalJatuhTempo"
							Required="true" />
						<FormFieldButtonLookup Name="KodeKategoriDebitur" Label="Kategori Debitur" Target="KategoriDebiturLookup"
							@bind-Value="@row.KodeKategoriDebitur" @bind-Text="@row.KodeKategoriDebiturDesc" Required="true" />
					</RadzenRow>

					<RadzenRow>
						<FormFieldButtonLookup Name="KodeJenisPenggunaan" Label="Jenis Penggunaan" Target="JenisPenggunaanLookup"
							@bind-Value="@row.KodeJenisPenggunaan" @bind-Text="@row.KodeJenisPenggunaanDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeOrientasiPenggunaan" Label="Orientasi Penggunaan"
							Target="OrientasiPenggunaanLookup" @bind-Value="@row.KodeOrientasiPenggunaan"
							@bind-Text="@row.KodeOrientasiPenggunaanDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeSektorEkonomi" Label="Sektor Ekonomi" Target="SektorEkonomiLookup"
							@bind-Value="@row.KodeSektorEkonomi" @bind-Text="@row.KodeSektorEkonomiDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeDati2" Label="Kode Dati II" Target="KodeDati2Lookup"
							@bind-Value="@row.KodeDati2" @bind-Text="@row.KodeDati2Desc" Required="true" />


						<FormFieldButtonLookup Name="KodeValuta" Label="Valuta" Target="ValutaLookup" @bind-Value="@row.KodeValuta"
							@bind-Text="@row.KodeValutaDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldNumeric Label="Presentase Suku Bunga" Name="PresentaseSukuBunga"
							@bind-Value="@row.PresentaseSukuBunga" Required="true" Format="#,#.00" />

						<FormFieldButtonLookup Name="JenisSukuBunga" Label="Jenis Suku Bunga" Target="JenisSukuBungaLookup"
							@bind-Value="@row.JenisSukuBunga" @bind-Text="@row.JenisSukuBungaDesc" Required="true" />
						<FormFieldButtonLookup Name="KreditProgramPemerintah" Label="Kredit Program Pemerintah"
							Target="KreditProgramPemerintahLookup" @bind-Value="@row.KreditProgramPemerintah"
							@bind-Text="@row.KreditProgramPemerintahDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="TakeoverDari" Label="Takeover Dari" Target="TakeoverDariLookup"
							@bind-Value="@row.TakeoverDari" @bind-Text="@row.TakeoverDariDesc" Required="true" />

						<FormFieldButtonLookup Name="SumberDana" Label="Sumber Dana" Target="SumberDanaLookup"
							@bind-Value="@row.SumberDana" @bind-Text="@row.SumberDanaDesc" Required="true" />
						<FormFieldNumeric Label="Plafon Awal" Name="PlafonAwal" @bind-Value="@row.PlafonAwal" Required="true"
							Format="#,#.00" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldNumeric Label="Plafon" Name="Plafon" @bind-Value="@row.Plafon" Required="true" Format="#,#.00" />

						<FormFieldNumeric Label="Realisasi Pencairan" Name="RealisasiPencairan" @bind-Value="@row.RealisasiPencairan"
							Required="true" Format="#,#.00" />
						<FormFieldNumeric Label="Denda" Name="Denda" @bind-Value="@row.Denda" Required="true" Format="N2" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldNumeric Label="Baki Debet" Name="BakiDebet" @bind-Value="@row.BakiDebet" Required="true"
							Format="#,#.00" />

						<FormFieldNumeric Label="Nilai Dalam Mata Uang Asal" Name="NilaiDalamMataUangAsal"
							@bind-Value="@row.NilaiDalamMataUangAsal" Required="true" Format="#,#.00" />
						<FormFieldButtonLookup Name="KodeKolektibilitas" Label="Kolektibilitas" Target="KolektibilitasLookup"
							@bind-Value="@row.KodeKolektibilitas" @bind-Text="@row.KodeKolektibilitasDesc" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldDatePicker Label="Tanggal Macet" Name="TanggalMacet" @bind-Value="@row.TanggalMacet"
							Required="true" />
						<FormFieldButtonLookup Name="KodeSebabMacet" Label="Sebab Macet" Target="SebabMacetLookup"
							@bind-Value="@row.KodeSebabMacet" @bind-Text="@row.KodeSebabMacetDesc" Required="true" />
						<FormFieldNumeric Label="Tunggakan Pokok" Name="TunggakanPokok" @bind-Value="@row.TunggakanPokok"
							Required="true" Format="#,#.00" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldNumeric Label="Tunggakan Bunga" Name="TunggakanBunga" @bind-Value="@row.TunggakanBunga"
							Required="true" Format="#,#.00" />
						<FormFieldNumeric Label="Jumlah Hari Tunggakan" Name="JumlahHariTunggakan"
							@bind-Value="@row.JumlahHariTunggakan" Required="true" Format="#,#.00" />
						<FormFieldNumeric Label="Frekuensi Tunggakan" Name="FrekuensiTunggakan" @bind-Value="@row.FrekuensiTunggakan"
							Required="true" Format="#,#.00" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldNumeric Label="Frekuensi Restrukturisasi" Name="FrekuensiRestrukturisasi"
							@bind-Value="@row.FrekuensiRestrukturisasi" Required="true" Format="#,#.00" />
						<FormFieldDatePicker Label="Tanggal Restrukturisasi Awal" Name="TanggalRestrukturisasiAwal"
							@bind-Value="@row.TanggalRestrukturisasiAwal" Required="true" />
						<FormFieldDatePicker Label="Tanggal Restrukturisasi Akhir" Name="TanggalRestrukturisasiAkhir"
							@bind-Value="@row.TanggalRestrukturisasiAkhir" Required="true" />
					</RadzenRow>
					<RadzenRow>
						<FormFieldButtonLookup Name="KodeCaraRestrukturisasi" Label="Cara Restrukturisasi"
							Target="CaraRestrukturisasiLookup" @bind-Value="@row.KodeCaraRestrukturisasi"
							@bind-Text="@row.KodeCaraRestrukturisasiDesc" Required="true" />
						<FormFieldButtonLookup Name="KodeKondisi" Label="Kondisi" Target="KondisiLookup" @bind-Value="@row.KodeKondisi"
							@bind-Text="@row.KodeKondisiDesc" Required="true" />
						<FormFieldDatePicker Label="Tanggal Kondisi" Name="TanggalKondisi" @bind-Value="@row.TanggalKondisi"
							Required="true" />
					</RadzenRow>
					<RadzenRow>

						<FormFieldTextArea Label="Keterangan" Name="Keterangan" @bind-Value="@row.Keterangan" Max="600"
							Required="true" />

						<FormFieldButtonLookup Name="KodeKantorCabang" Label="Kantor Cabang" Target="KantorCabangLookup"
							@bind-Value="@row.KodeKantorCabang" @bind-Text="@row.SysCompanyKantorCabang" Required="true" />
						<FormFieldButtonLookup Name="OperasiData" Label="Operasi Data" Target="OperasiDataLookup"
							@bind-Value="@row.OperasiData" @bind-Text="@row.OperasiDataDesc" Required="true" />
					</RadzenRow>
				}
			</RadzenStack>
		</RadzenStack>
	</RadzenTemplateForm>
}
<SingleSelectLookup @ref="@KantorCabangLookup" TItem="SysCompanyModel" LoadData="LoadKantorCabangLookup"
	Title="Kantor Cabang List" Select="(select) => {
								row.KodeKantorCabang        = select.Code;
								row.SysCompanyKantorCabang = select.Name;
							}">
	<RadzenDataGridColumn TItem="SysCompanyModel" Property="Code" Title="Code" />
	<RadzenDataGridColumn TItem="SysCompanyModel" Property="Name" Title="Name" />
</SingleSelectLookup>

